COPY ~%MOD_FOLDER%/analyze/spells~ OVERRIDE

// ANALYZE_AC
APPEND ~splprot.2da~ ~ANALYZE_AC 2 -1 1
ANALYZE_THAC0 7 -1 1
ANALYZE_SAVEVSDEATH 9 -1 1
ANALYZE_SAVEVSWANDS 10 -1 1
ANALYZE_SAVEVSPOLY 11 -1 1
ANALYZE_SAVEVSBREATH 12 -1 1
ANALYZE_SAVEVSSPELL 13 -1 1
ANALYZE_LEVEL 34 -1 1
ANALYZE_BACKSTABIMMUNITY 175 -1 1~ UNLESS ~ANALYZE_AC~
COPY_EXISTING ~splprot.2da~ OVERRIDE
	COUNT_2DA_COLS cols // amount of columns
	READ_2DA_ENTRIES_NOW rows cols // read all file into memory  
	FOR (row = 1; row < rows; ++row) BEGIN // iterate over rows
	  READ_2DA_ENTRY_FORMER rows row 0 ~stat~ // read column value
	  PATCH_IF ~%stat%~ STRING_EQUAL_CASE ~ANALYZE_AC~ BEGIN
	    SET ANALYZE_AC = %row%
	  END
	  PATCH_IF ~%stat%~ STRING_EQUAL_CASE ~ANALYZE_THAC0~ BEGIN
	    SET ANALYZE_THAC0 = %row%
	  END
	  PATCH_IF ~%stat%~ STRING_EQUAL_CASE ~ANALYZE_SAVEVSDEATH~ BEGIN
	    SET ANALYZE_SAVEVSDEATH = %row%
	  END
	  PATCH_IF ~%stat%~ STRING_EQUAL_CASE ~ANALYZE_SAVEVSWANDS~ BEGIN
	    SET ANALYZE_SAVEVSWANDS = %row%
	  END
	  PATCH_IF ~%stat%~ STRING_EQUAL_CASE ~ANALYZE_SAVEVSPOLY~ BEGIN
	    SET ANALYZE_SAVEVSPOLY = %row%
	  END
	  PATCH_IF ~%stat%~ STRING_EQUAL_CASE ~ANALYZE_SAVEVSBREATH~ BEGIN
	    SET ANALYZE_SAVEVSBREATH = %row%
	  END
	  PATCH_IF ~%stat%~ STRING_EQUAL_CASE ~ANALYZE_SAVEVSSPELL~ BEGIN
	    SET ANALYZE_SAVEVSSPELL = %row%
	  END
	  PATCH_IF ~%stat%~ STRING_EQUAL_CASE ~ANALYZE_LEVEL~ BEGIN
	    SET ANALYZE_LEVEL = %row%
	  END
	  PATCH_IF ~%stat%~ STRING_EQUAL_CASE ~ANALYZE_BACKSTABIMMUNITY~ BEGIN
	    SET ANALYZE_BACKSTABIMMUNITY = %row%
	  END
	  END

// Resistances
DEFINE_ACTION_FUNCTION	ADD_SPLPROT	INT_VAR	stat = 0	value = 0	relation = 0	RET	new_row		BEGIN
	ACTION_IF	(stat > 278)	BEGIN	WARN	~SPLPROT.2DA - Stat out of Bounds.~	END
	ACTION_IF	((stat != 259) AND (stat != 260) AND (relation > 11))	BEGIN	WARN	~SPLPROT.2DA - Relation out of Bounds.~	END
	COPY_EXISTING ~SPLPROT.2DA~	OVERRIDE
		COUNT_2DA_COLS	cols
		COUNT_2DA_ROWS cols rows
		TEXT_SPRINT check_row ~~
		TEXT_SPRINT check_stat ~~
		SET new_row = 0
		FOR(current_row = 0; current_row < rows; ++current_row)	BEGIN	READ_2DA_ENTRY current_row 1 cols check_stat
			PATCH_IF (~%check_stat%~ STRING_EQUAL ~%stat%~)	BEGIN	READ_2DA_ENTRY current_row 2 cols check_value
				PATCH_IF (~%check_value%~ STRING_EQUAL ~%value%~)	BEGIN	READ_2DA_ENTRY current_row 3 cols check_relation
					PATCH_IF (~%check_relation%~ STRING_EQUAL ~%relation%~)	BEGIN	new_row = current_row	current_row = rows	END
				END
			END
		END
		PATCH_IF (new_row = 0)	BEGIN
			FOR(current_row = 0; current_row < rows; ++current_row)	BEGIN	READ_2DA_ENTRY current_row 1 cols check_stat
				PATCH_IF (~%check_stat%~ STRING_EQUAL ~*~)	BEGIN	new_row = current_row	current_row = rows	END
			END
			PATCH_IF (new_row = 0)	BEGIN	new_row = rows	INSERT_2DA_ROW rows cols ~%new_row%		   %stat%		   %value%		   %relation%~
			END	ELSE	BEGIN	SET_2DA_ENTRY new_row 1 cols ~%stat%~	SET_2DA_ENTRY new_row 2 cols ~%value%~	SET_2DA_ENTRY new_row 3 cols ~%relation%~	END
		END
	BUT_ONLY
END

