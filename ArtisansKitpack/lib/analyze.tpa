ACTION_DEFINE_ASSOCIATIVE_ARRAY	DMG_ARRAY_NEG	BEGIN
	~CRUSHNEG~		=>	22
	~ACIDNEG~		=>	17
	~COLDNEG~		=>	15
	~ELECNEG~		=>	16
	~FIRENEG~		=>	14
	~PIERCNEG~		=>	23
	~POISNNEG~		=>	74
	~MAGICNEG~		=>	73
	~MSSLNEG~		=>	24
	~SLASHNEG~		=>	21
	~MFIRENEG~		=>	19
	~MCOLDNEG~		=>	20
END
ACTION_PHP_EACH	DMG_ARRAY_NEG AS one => two	BEGIN
	LAF	ADD_SPLPROT	INT_VAR	stat = two	value = %-128%	relation = 	2	RET	min = new_row	END
	LAF	ADD_SPLPROT	INT_VAR	stat = two	value = 1	relation = 	4	RET	max = new_row	END
	LAF	ADD_SPLPROT	INT_VAR	stat = 260	value = min	relation = 	max	RET	EVAL ~%one%~ = new_row	END
END

